ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /home/shenchi/anaconda3/envs/experiment/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /home/shenchi/anaconda3/envs/experiment/include -I/home/shenchi/anaconda3/envs/experiment/include -fPIC -O2 -isystem /home/shenchi/anaconda3/envs/experiment/include -fPIC -I/home/shenchi/anaconda3/envs/experiment/lib/python3.9/site-packages/torch/include -I/home/shenchi/anaconda3/envs/experiment/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/home/shenchi/anaconda3/envs/experiment/lib/python3.9/site-packages/torch/include/TH -I/home/shenchi/anaconda3/envs/experiment/lib/python3.9/site-packages/torch/include/THC -I/usr/local/cuda/include -I/home/shenchi/anaconda3/envs/experiment/include/python3.9 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=fw_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/home/shenchi/anaconda3/envs/experiment/lib/python3.9/site-packages/torch/include -I/home/shenchi/anaconda3/envs/experiment/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/home/shenchi/anaconda3/envs/experiment/lib/python3.9/site-packages/torch/include/TH -I/home/shenchi/anaconda3/envs/experiment/lib/python3.9/site-packages/torch/include/THC -I/usr/local/cuda/include -I/home/shenchi/anaconda3/envs/experiment/include/python3.9 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=fw_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_75,code=compute_75 -gencode=arch=compute_75,code=sm_75 -std=c++14
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



build /data/shenchi/experiment/code/cuda/build/temp.linux-x86_64-3.9/fw_cuda.o: compile /data/shenchi/experiment/code/cuda/fw_cuda.cpp
build /data/shenchi/experiment/code/cuda/build/temp.linux-x86_64-3.9/fw_cuda_kernel.o: cuda_compile /data/shenchi/experiment/code/cuda/fw_cuda_kernel.cu





